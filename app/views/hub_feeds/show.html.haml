- content_for :top_panel do
  = render partial: 'top_panel', locals: { hub: @hub, hub_feed: @hub_feed }
- content_for :tabs do
  -if @user
    = render partial: 'users/hub_items_tabs', locals: { active: 'about' }
  - else
    = render partial: 'tabs', locals: { active: 'about' }
- content_for :tab_content do
  = render partial: 'about', locals: { hub: @hub, hub_feed: @hub_feed }
